<h1>Eat-da-Burger </h1>
<img src="../public/assets/img/burger.jpg">
<ul>
    {{#each burgers}}
    {{#unless devoured}}
    {{/mo}}
    <li>
        <p>
            {{this.id}}. {{this.plan}}
            <button data-planid="{{this.id}}" class="addPlan">Add Burger</button>
        </p>
    </li>
    {{/unless}}
    {{/each}}
</ul>
